{
  "name": "reportscript",
  "version": "0.0.0-development",
  "description": "report generation library",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist/",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "test": "jest",
    "semantic-release": "semantic-release --branches main",
    "lint": "npx eslint src",
    "lint:fix": "npm run lint -- --fix --color",
    "prettier": "npx prettier src --check",
    "prettier:fix": "npm run prettier -- --write",
    "format": "npm run prettier:fix && npm run lint:fix",
    "commit": "cz",
    "build": "tsup ./src/index.ts --format cjs,esm --dts"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/canyon-trail/reportscript"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/canyon-trail/reportscript/issues"
  },
  "homepage": "https://github.com/canyon-trail/reportscript#readme",
  "devDependencies": {
    "@types/jest": "^27.0.4",
    "@typescript-eslint/eslint-plugin": "^4.28.3",
    "@typescript-eslint/parser": "^4.28.3",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.30.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-deprecation": "^1.4.1",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "husky": "^8.0.3",
    "jest": "^27.0.6",
    "prettier": "2.8.7",
    "semantic-release": "^22.0.7",
    "semantic-release-cli": "^5.4.4",
    "sinon": "^12.0.1",
    "ts-jest": "^27.0.4",
    "tslint-config-prettier": "^1.18.0",
    "tsup": "^8.0.1"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@types/lodash": "^4.14.178",
    "@types/node": "^16.3.2",
    "@types/pdfkit": "^0.11.2",
    "blob-stream": "^0.1.3",

    "lodash": "^4.17.21",
    "pdfkit": "^0.12.1",
    "ts-node": "^10.1.0",
    "typescript": "^4.3.5"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*.(tsx|ts)": [
      "prettier --check --write",
      "eslint"
    ]
  }
}
